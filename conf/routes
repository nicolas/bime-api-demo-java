# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET        /                       controllers.Bime.actions()
GET        /dashboards             controllers.Bime.dashboards()
GET        /connections            controllers.Bime.connections()
GET        /connections/:id        controllers.Bime.connection(id: Int)
GET        /workflow               controllers.Bime.workflow()